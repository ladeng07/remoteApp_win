<Window x:Class="IconDisplayApp.ShortcutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="桌面图标" Height="400" Width="600">

    <TabControl>
        <TabItem Header="公共桌面">
        <ScrollViewer>
        <WrapPanel x:Name="ShortcutWrapPanel"/>
    </ScrollViewer>
        </TabItem>
        <TabItem Header="用户桌面">
            <ScrollViewer>
                <WrapPanel x:Name="ShortcutWrapPanel2"/>
            </ScrollViewer>
        </TabItem>
    </TabControl>
</Window>
